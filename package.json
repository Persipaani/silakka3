{
  "name": "silakka3",
  "version": "0.0.1",
  "repository": "https://github.com/Persipaani/silakka3.git",
  "scripts": {
    "start": "cross-env NODE_ENV=production babel-node src/index.js",
    "start:dev": "cross-env NODE_ENV=development babel-node src/index.js",
    "start:watch": "nodemon --exec npm run start:dev",
    "test": "cross-env NODE_ENV=test jest --forceExit",
    "test:coverage": "npm run test -- --coverage"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "package.json": [
      "format-package -w",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "chalk": "^2.4.2",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "node-telegram-bot-api": "^0.30.0",
    "query-string": "^6.5.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "babel-plugin-root-import": "^6.2.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.0",
    "telegram-test-api": "^2.2.1"
  },
  "optionalDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "format-package": "^4.5.0",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7"
  }
}
